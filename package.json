{
  "name": "gtfs-to-geojson",
  "description": "Generate geoJSON of transit route data from a GTFS file",
  "version": "0.4.5",
  "keywords": [
    "transit",
    "gtfs",
    "transportation",
    "geojson"
  ],
  "private": false,
  "author": "Brendan Nee <brendan@blinktag.com>",
  "contributors": [],
  "repository": "git://github.com/blinktaginc/gtfs-to-geojson",
  "bugs": {
    "url": "https://github.com/blinktaginc/gtfs-to-geojson/issues"
  },
  "license": "MIT",
  "scripts": {
    "lint": "xo"
  },
  "bin": {
    "gtfs-to-geojson": "bin/gtfs-to-geojson.js"
  },
  "main": "index",
  "dependencies": {
    "@mapbox/geojson-merge": "^1.0.2",
    "archiver": "^2.1.0",
    "cli-table": "^0.3.1",
    "fs-extra": "^5.0.0",
    "gtfs": "1.3.3",
    "lodash": "^4.17.4",
    "mongoose": "5.0.0-rc2",
    "sanitize-filename": "^1.6.1",
    "timer-machine": "^1.1.0",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "xo": "0.18.2"
  },
  "engines": {
    "node": ">= 8.1.4"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ]
    }
  }
}
