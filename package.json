{
  "name": "gtfs-to-geojson",
  "description": "Generate geoJSON of transit route data from a GTFS file",
  "version": "1.0.4",
  "keywords": [
    "transit",
    "gtfs",
    "transportation",
    "geojson"
  ],
  "private": false,
  "author": "Brendan Nee <brendan@blinktag.com>",
  "contributors": [],
  "repository": "git://github.com/blinktaginc/gtfs-to-geojson",
  "bugs": {
    "url": "https://github.com/blinktaginc/gtfs-to-geojson/issues"
  },
  "license": "MIT",
  "scripts": {
    "lint": "xo"
  },
  "bin": {
    "gtfs-to-geojson": "bin/gtfs-to-geojson.js"
  },
  "main": "index",
  "dependencies": {
    "@turf/simplify": "^5.1.5",
    "archiver": "^5.0.2",
    "chalk": "^4.1.0",
    "cli-table": "^0.3.1",
    "fs-extra": "^9.0.1",
    "gtfs": "^2.0.7",
    "lodash": "^4.17.20",
    "pretty-error": "^2.1.1",
    "sanitize-filename": "^1.6.3",
    "timer-machine": "^1.1.0",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "xo": "0.33.1"
  },
  "engines": {
    "node": ">= 12.14.0"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  }
}
